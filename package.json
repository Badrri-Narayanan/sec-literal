{
  "name": "sec-literal",
  "version": "1.0.0",
  "description": "Package created to analyze JavaScript literals",
  "types": "./index.d.ts",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js",
    "test": "jest --coverage && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fraxken/sec-literal.git"
  },
  "keywords": [
    "security",
    "literal",
    "estree",
    "analysis",
    "scanner"
  ],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/"
    ]
  },
  "bugs": {
    "url": "https://github.com/fraxken/sec-literal/issues"
  },
  "homepage": "https://github.com/fraxken/sec-literal#readme",
  "devDependencies": {
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/is": "^1.5.1",
    "@types/node": "^14.14.2",
    "jest": "^26.6.1"
  },
  "dependencies": {
    "is-base64": "^1.1.0",
    "is-svg": "^4.2.1",
    "string-width": "^4.2.0"
  }
}
